{% extends '::base.html.twig' %}

{% block body -%}
    <h1>EmpleadoCliente</h1>

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Fechaasignacion</th>
                <td>{{ entity.fechaAsignacion|date('Y-m-d H:i:s') }}</td>
            </tr>
            <tr>
                <th>Estado</th>
                <td>{{ entity.estado }}</td>
            </tr>
            <tr>
                <th>Metavisitasmes</th>
                <td>{{ entity.metaVisitasMes }}</td>
            </tr>
            <tr>
                <th>Metaventasmes</th>
                <td>{{ entity.metaVentasMes }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('empleadocliente') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('empleadocliente_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
{% endblock %}
