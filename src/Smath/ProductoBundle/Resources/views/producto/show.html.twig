{% extends '::base.html.twig' %}

{% block body -%}
    <h1>producto</h1>

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Proreferencia</th>
                <td>{{ entity.proReferencia }}</td>
            </tr>
            <tr>
                <th>Pronombre</th>
                <td>{{ entity.proNombre }}</td>
            </tr>
            <tr>
                <th>Prodescripcion</th>
                <td>{{ entity.proDescripcion }}</td>
            </tr>
            <tr>
                <th>Proactivo</th>
                <td>{{ entity.proActivo }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('producto') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('producto_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
{% endblock %}
