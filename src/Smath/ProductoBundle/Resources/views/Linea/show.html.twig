{% extends '::base.html.twig' %}

{% block body -%}
    <h1>Linea</h1>

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Codigo</th>
                <td>{{ entity.Codigo }}</td>
            </tr>
            <tr>
                <th>Descripcion</th>
                <td>{{ entity.Descripcion }}</td>
            </tr>
            <tr>
                <th>Lineaid</th>
                <td>{{ entity.lineaId }}</td>
            </tr>
            <tr>
                <th>Estado</th>
                <td>{{ entity.Estado }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('linea') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('linea_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
{% endblock %}
